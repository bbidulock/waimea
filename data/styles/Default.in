menu.title:             flat gradient crossdiagonal
  menu.title.color:     #8090c0
  menu.title.colorTo:   #405070
  menu.title.textColor: #ffffff

menu.frame:             flat gradient crossdiagonal
  menu.frame.color:     #ffffff
  menu.frame.colorTo:   #8090c0
  menu.frame.textColor: #000000

menu.hilite:             flat gradient horizontal
  menu.hilite.color:     #606878
  menu.hilite.colorTo:   #000000
  menu.hilite.textColor: #e0f0ff

menu.bullet.look:         >
menu.justify:             right
menu.checkbox.true.look:  [x]
menu.checkbox.false.look: [ ]

window.title.focus:           flat gradient crossdiagonal
  window.title.focus.color:   #7080b0
  window.title.focus.colorTo: #ffffff
  
window.title.unfocus:           flat gradient crossdiagonal
  window.title.unfocus.color:   #7080b0
  window.title.unfocus.colorTo: #ffffff
  window.title.unfocus.opacity: 50
  
window.justify: right

window.label.focus:             parentrelative
  window.label.focus.textColor: #000000
  
window.label.unfocus:                     parentrelative
  window.label.unfocus.textColor:         #222222
  window.label.unfocus.textColor.opacity: 50

window.handle.focus:           flat gradient crossdiagonal
  window.handle.focus.color:   #90b098
  window.handle.focus.colorTo: #506058
  
window.handle.unfocus:           flat gradient crossdiagonal
  window.handle.unfocus.color:   #90b098
  window.handle.unfocus.colorTo: #506058
  window.handle.unfocus.opacity: 50

window.grip.focus:           flat gradient crossdiagonal
  window.grip.focus.color:   #fff0a0
  window.grip.focus.colorTo: #c0a040
  
window.grip.unfocus:           flat gradient crossdiagonal
  window.grip.unfocus.color:   #fff0a0
  window.grip.unfocus.colorTo: #c0a040
  window.grip.unfocus.opacity: 50

window.button0.foreground:           True
window.button0.state:                SHADED
window.button0.autoplace:            Left
window.button0.position:             0
window.button0.*.focus:              parentrelative
  window.button0.*.focus.picColor:   #000000
window.button0.*.unfocus:            parentrelative
  window.button0.*.unfocus.picColor: #000000
window.button0.*.pressed:            parentrelative
  window.button0.*.pressed.picColor: #506058

window.button1.foreground:           True
window.button1.state:                CLOSE
window.button1.autoplace:            Right
window.button1.position:             0
window.button1.*.focus:              parentrelative
  window.button1.*.focus.picColor:   #000000
window.button1.*.unfocus:            parentrelative
  window.button1.*.unfocus.picColor: #000000
window.button1.*.pressed:            parentrelative
  window.button1.*.pressed.picColor: #506058

window.button2.foreground:           True
window.button2.state:                MAXIMIZED
window.button2.autoplace:            Right
window.button2.position:             0
window.button2.*.focus:              parentrelative
  window.button2.*.focus.picColor:   #000000
window.button2.*.unfocus:            parentrelative
  window.button2.*.unfocus.picColor: #000000
window.button2.*.pressed:            parentrelative
  window.button2.*.pressed.picColor: #506058

dockappholder.*.frame:           raised gradient crossdiagonal
  dockappholder.*.frame.color:   #7080b0
  dockappholder.*.frame.colorTo: #405080

dockappholder.*.borderWidth: 2
dockappholder.*.borderColor: #000000

borderColor: #000000
borderWidth: 2
handleWidth: 3

! outlineColor:      #ffffff
! menu.borderWidth:  2
! menu.title.height: 15
! menu.item.height:  15

! *.font:        arial:pixelsize=12 [xft]

rootCommand: Esetroot -s @pkgdatadir@/backgrounds/waimea.png
